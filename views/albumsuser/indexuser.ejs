<h2 class="page-header">Search Album</h2>
<form action="/albumsuser" method="GET">
  <div class="form-row">
    <div class="form-item">
      <label>Title</label>
      <input type="text" name="title" value="<%= locals.searchOptions.title %>" />
    </div>
  </div>
  <div class="form-row">
    <div class="form-item">
      <label>Published After</label>
      <input type="date" name="publishedAfter" value="<%= locals.searchOptions.publishedAfter %>" />
    </div>
    <div class="form-item">
      <label>Published Before</label>
      <input type="date" name="publishedBefore" value="<%= locals.searchOptions.publishedBefore %>" />
    </div>
  </div>
  <div class="form-row form-row-end">
    <a class="btn btn-primary" href="/indexuser">Back</a>
    <button class="btn btn-primary" type="submit>">Submit</button>
  </div>
</form>
<br>
<%- include('../partials/bookGrid3', { albums: albums, large: true})%>
<div class="form-row form-row-end">
</div>